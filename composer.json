{
  "name": "ahid-technologies/leopards-courier",
  "description": "Laravel package for integrating with Leopards Courier API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ahid Technologies",
      "email": "info@ahidtechnologies.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^7.4|^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0",
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Ahid\\LeopardsCourier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ahid\\LeopardsCourier\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ahid\\LeopardsCourier\\AhidLeopardsServiceProvider"
      ],
      "aliases": {
        "LeopardsCourier": "Ahid\\LeopardsCourier\\Facades\\LeopardsCourier"
      }
    }
  }
}
